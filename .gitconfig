[core]
	editor = vim
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	required = true
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
[pull]
	default = current
[push]
	default = current
[alias]
	ca = commit --amend --no-edit
	ds = diff --staged
	s = status --short --branch
        c = commit
	a = add
	co = checkout
	com = checkout master
	new = checkout -b
	rh = reset --hard
	b = branch
	recent = for-each-ref --sort='authordate:iso8601' --format=' %(authordate:relative)%09%(refname:short)' refs/heads
	la = "!git config -l | grep alias | cut -c 7-"
	sco = !sh -c \"git branch -a | grep -v remotes | grep $1 | xargs git checkout\"
	cos = !sh -c \"git sco SPRINT\"
[commit]
	gpgsign = true
[gpg]
	program = gpg
